{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "off",
        "quotes": [2, "double", { "avoidEscape": true }],
        "no-use-before-define": "off",
        "no-shadow": "off",
        "no-console": "off"
    }
}
